    "webpack-dev-server": "2.9.4",
    "webpack-notifier": "1.5.0"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx,js,json,css,scss}": ["prettier --write", "git add"]
    }
  }
}
