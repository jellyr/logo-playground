{
  "name": "logo-sandbox",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Logo Sandbox",
  "scripts": {
    "precommit": "lint-staged",
    "build-vendor-dev": "webpack  --config webpack.config.vendor.js",
    "build-app-dev": "webpack  --config webpack.config.js",
    "build-vendor-prod": "webpack  --config webpack.config.vendor.js --env.prod",
    "build-app-prod": "webpack  --config webpack.config.js --env.prod",
    "build": "npm run build-vendor-prod && npm run build-app-prod",
    "test": "npm run build-vendor-dev && karma start karma.conf.js",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage-remap && npm run coverage-report-html && npm run coverage-report-clover",
    "coverage-remap": "remap-istanbul -i reports/coverage/coverage.json -o reports/coverage/coverage.json -t json",
    "coverage-report-html": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage html",
    "coverage-report-clover": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage clover",
    "start": "npm run build-vendor-dev && webpack-dev-server",
    "start-tslint": "npm run build-vendor-dev && webpack-dev-server --env.tslint_enabled=true",
    "build-war": "node tools/make-war.js",
    "utr": "tsc tools/update-translations.ts --lib dom,es6 --target es5 && node tools/update-translations.js"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clipboard": "1.7.1",
    "codemirror": "5.31.0",
    "color": "2.0.1",
    "core-js": "2.5.1",
    "crypto-js": "3.1.9-1",
    "file-saver": "1.3.3",
    "golden-layout": "1.5.9",
    "inversify": "4.5.1",
    "inversify-inject-decorators": "3.0.2",
    "isomorphic-fetch": "2.2.1",
    "jed": "1.1.1",
    "jquery": "3.2.1",
    "keymaster": "1.6.2",
    "markdown-it": "8.4.0",
    "moment": "2.19.2",
    "po2json": "0.4.5",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "spark-md5": "3.0.0",
    "tslib": "1.8.0",
    "tv4": "1.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.5",
    "@types/chai-as-promised": "7.1.0",
    "@types/classnames": "2.2.3",
    "@types/clipboard": "1.5.35",
    "@types/codemirror": "0.0.51",
    "@types/color": "2.0.0",
    "@types/core-js": "0.9.43",
    "@types/crypto-js": "3.1.37",
    "@types/enzyme": "3.1.4",
    "@types/enzyme-adapter-react-16": "1.0.1",
    "@types/esprima": "4.0.1",
    "@types/file-saver": "1.3.0",
    "@types/glob": "5.0.33",
    "@types/history": "4.6.2",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jquery": "3.2.16",
    "@types/keymaster": "1.6.28",
    "@types/markdown-it": "0.0.4",
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.53",
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "@types/react-router": "4.0.17",
    "@types/react-router-dom": "4.2.1",
    "@types/react-test-renderer": "16.0.0",
    "@types/tv4": "1.2.28",
    "archiver": "2.1.0",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier": "1.8.2",
    "awesome-typescript-loader": "3.4.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.2.1",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "esprima": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
