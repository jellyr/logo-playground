    "po2json": "0.4.5",
    "react": "15.5.4",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.5.4",
    "react-router": "3.0.5",
    "react-router-bootstrap": "0.23.1",
    "react-select": "1.0.0-rc.3",
    "rxjs": "5.3.0",
    "spark-md5": "3.0.0",
    "tslib": "1.6.0",
    "tv4": "1.3.0"
  },
