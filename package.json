{
  "name": "logo-sandbox",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Logo Sandbox",
  "scripts": {
    "build-vendor-dev": "webpack  --config webpack.config.vendor.js",
    "build-app-dev": "webpack  --config webpack.config.js",
    "build-vendor-prod": "webpack  --config webpack.config.vendor.js --env.prod",
    "build-app-prod": "webpack  --config webpack.config.js --env.prod",
    "build": "npm install && npm run build-vendor-prod && npm run build-app-prod",
    "test": "karma start karma.conf.js",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage-remap && npm run coverage-report-html && npm run coverage-report-clover",
    "coverage-remap": "remap-istanbul -i reports/coverage/coverage.json -o reports/coverage/coverage.json -t json",
    "coverage-report-html": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage html",
    "coverage-report-clover": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage clover",
    "start": "npm run build-vendor-dev && webpack-dev-server",
    "build-war": "node tools/make-war.js",
    "utr": "tsc tools/update-translations.ts --lib dom,es6 --target es5 && node tools/update-translations.js"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "codemirror": "5.25.0",
